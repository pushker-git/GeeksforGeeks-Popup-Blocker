{
  "name": "GeeksforGeeks Popup Blocker",
  "description": "Block annoying login popup in geeksforgeeks website",
  "version": "1.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": ["https://*.geeksforgeeks.org/*"],
      "js": ["content-script.js"]
    }
  ],
  "icons": {
    "16": "icon16.png",
    "128": "icon128.png"
  }
}
